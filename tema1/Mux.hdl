/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a= sel, b= sel, out= nsel);
    Nand(a= a, b= nsel, out= o1);
    Nand(a= b, b= sel, out= o2);
    Nand(a= o1, b= o2, out= out);
}